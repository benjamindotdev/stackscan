{
  "name": "stacksync",
  "version": "0.1.0",
  "description": "Automatically detect tech stacks from repositories and generate logos, JSON, and markdown for portfolios.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stacksync": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src --format cjs,esm --dts",
    "dev": "tsup src --watch",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "tech-stack",
    "cli",
    "portfolio",
    "scanner",
    "typescript",
    "logos"
  ],
  "author": "Benjamin Elliott",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cosmiconfig": "^9.0.0",
    "devicon": "^2.17.0",
    "devicons": "^1.8.0",
    "fast-glob": "^3.3.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "lucide-static": "^0.555.0",
    "ora": "^7.0.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "simple-icons": "^15.22.0",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3"
  }
}
